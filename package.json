{
  "name": "bamboo",
  "version": "0.0.1",
  "description": "React base UI component library",
  "scripts": {
    "test": "jest",
    "eslint": "eslint src components",
    "eslintFix": "eslint --fix src components",
    "vendor": "webpack --config vendor.webpack.config.js",
    "grunt": "grunt",
    "dist": "cross-env NODE_ENV=production babel ./components -d ./dist/js && grunt",
    "pages": "npm run vendor && cross-env NODE_ENV=production webpack --config githubPages.webpack.config.js",
    "start": "cross-env NODE_ENV=development pm2 start ./scripts/server-register.js --name=bamboo --env development",
    "stop": "pm2 stop bamboo && pm2 delete bamboo",
    "restart": "npm stop && npm start"
  },
  "main": "dist/js/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:zombiej/bamboo.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "zombiej",
  "license": "Apache2",
  "bugs": {
    "url": "https://github.com/zombiej/bamboo/issues"
  },
  "homepage": "https://github.com/zombiej/bamboo#readme",
  "dependencies": {
    "prop-types": ">15.5.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "admin-lte": "^2.3.8",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "easy-stop": "0.0.5",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "fs-extra": "^0.30.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-sass": "^1.0.0",
    "highcharts": "^5.0.14",
    "immutable": "^3.8.1",
    "jest": "^21.2.1",
    "jquery": "^3.1.1",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "pm2": "^2.4.1",
    "postcss-loader": "^1.1.1",
    "react": "^16.0.0",
    "react-addons-perf": "^15.4.0",
    "react-css-modules": "^3.7.10",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "remarkable": "^1.7.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-jest": "^21.1.2",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.7",
    "warning": "^3.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  }
}
