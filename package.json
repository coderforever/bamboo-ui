{
  "name": "bamboo-ui",
  "version": "0.0.59",
  "description": "React base UI component library",
  "scripts": {
    "build": "node ./scripts/build.js",
    "prepublishOnly": "npm run build",

    "test": "jest components",
    "eslint": "eslint src components",
    "eslintFix": "eslint --fix src components",
    "vendor": "webpack --config vendor.webpack.config.js",
    "dist": "cross-env NODE_ENV=production babel ./components -d ./dist/js -s && grunt",
    "pages": "cross-env NODE_ENV=production npm run vendor && cross-env NODE_ENV=production webpack --config githubPages.webpack.config.js",
    "start": "cross-env NODE_ENV=development pm2 start ./scripts/server-register.js --name=bamboo --env development",
    "stop": "pm2 stop bamboo && pm2 delete bamboo",
    "restart": "npm stop && npm start"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:zombiej/bamboo.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "zombiej",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zombiej/bamboo/issues"
  },
  "homepage": "https://github.com/zombiej/bamboo#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.2",
    "css-loader": "^0.28.7",
    "easy-stop": "0.0.5",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.6.3",
    "fs-extra": "^0.30.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-sass": "^1.0.0",
    "highcharts": "^5.0.14",
    "immutable": "^3.8.1",
    "jest": "^21.2.1",
    "jquery": "^3.1.1",
    "moment": "^2.18.1",
    "node-sass": "^4.6.0",
    "pm2": "^2.4.1",
    "postcss-loader": "^2.0.8",
    "prop-types": ">15.5.0",
    "react": "^16.0.0",
    "react-css-modules": "^3.7.10",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "remarkable": "^1.7.1",
    "rollup": "^0.51.5",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "warning": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
